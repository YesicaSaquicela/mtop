<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.0" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.4.1
                                   		http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.4.1.xsd">

    <rewrite match=".*/" trailingSlash="remove"/>
   
    <!-- Content Pages -->
   
    <url-mapping id="home">
        <pattern value="/"></pattern>
        <view-id value="/paginas/inicio.xhtml"></view-id>
    </url-mapping> 
   
    <url-mapping id="loggedOffHome">
        <pattern value="/"></pattern>
        <view-id value="/pages/loggedOffHome.xhtml"></view-id>
    </url-mapping> 
   
    <url-mapping id="login">
        <pattern value="/"></pattern>
        <view-id value="/paginas/login.xhtml"></view-id>
    </url-mapping> 
   
         
    <!-- Begin Exception Mappings -->
    <url-mapping id="404">
        <pattern value="/404" />
        <view-id value="/pages/404.xhtml" />
    </url-mapping>
    <url-mapping id="error">
        <pattern value="/error" />
        <view-id value="/pages/error.xhtml" />
    </url-mapping>
    <url-mapping id="kardex">
        <pattern value="/kardex/lista" />
        <view-id value="/paginas/kardex/lista.xhtml" />
        
    </url-mapping>
    
    <url-mapping id="solicitud">
        <pattern value="/solicitud/lista" />
        <view-id value="/paginas/secretario/solicitud/lista.xhtml" />
    </url-mapping>
    
    <!-- End Exception Mappings -->
    
    <url-mapping id="changePassword">
        <pattern value="/reset"></pattern>
        <view-id value="/pages/reset.xhtml"></view-id>
      
    </url-mapping>


</pretty-config>
